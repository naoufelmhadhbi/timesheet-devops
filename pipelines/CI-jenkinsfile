pipeline {
agent any
stages {
stage ('GIT') {
steps {
echo "Getting Project from Git";
git "https://github.com/naoufelmhadhbi/timesheet-devops.git";
}
}
stage ('MVN CLEAN') {
steps {
echo "Maven Clean";
bat 'mvn clean';
}
}
stage ('MVN TEST') {
steps {
echo "Maven Test JUnit";
bat 'mvn test';
}
}
}
}